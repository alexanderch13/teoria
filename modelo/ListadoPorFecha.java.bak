
package modelo;

import java.util.List;
import java.util.stream.Collectors;

public class ListadoPorFecha implements EstrategiaListado {

    @Override
    public List<String> generarSalida(List<ParAsociativo<Equipo, Mantenimiento>> pares) {
        return pares.stream()
                .sorted((a,b) -> a.getSegundo().getFecha().compareTo(b.getSegundo().getFecha()))
                .map(x -> x.toString())
                .collect(Collectors.toList());
    }
}
