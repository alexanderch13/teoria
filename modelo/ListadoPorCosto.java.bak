
package modelo;

import java.util.List;
import java.util.stream.Collectors;

public class ListadoPorCosto implements EstrategiaListado {

    @Override
    public List<String> generarSalida(List<ParAsociativo<Equipo, Mantenimiento>> pares) {
        return pares.stream()
                .sorted((a,b) -> Double.compare(a.getSegundo().getCosto(), b.getSegundo().getCosto()))
                .map(x -> x.toString())
                .collect(Collectors.toList());
    }
}
